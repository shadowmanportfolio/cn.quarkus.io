#.  tag::quickly-install[]
#. type: Plain text
#: upstream/_versions/main/guides/includes/compile-quarkus-quickly.adoc:3
msgid "The following will build all modules in the Quarkus repository except docs and test modules and install them in your local maven repository with the `999-SNAPSHOT` version:"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/includes/compile-quarkus-quickly.adoc:10
#, no-wrap
msgid ""
"$ ./mvnw clean install -Dquickly \\ #<1>\n"
"  -DskipTests \\ # <2>\n"
"  -DskipITs \\ # <3>\n"
"  -Dno-test-modules # <4>\n"
msgstr ""

#. type: Plain text
#: upstream/_versions/main/guides/includes/compile-quarkus-quickly.adoc:12
#: upstream/_versions/main/guides/includes/compile-quarkus-quickly.adoc:29
msgid "`-Dquickly` skips building documentation"
msgstr ""

#. type: Plain text
#: upstream/_versions/main/guides/includes/compile-quarkus-quickly.adoc:13
#: upstream/_versions/main/guides/includes/compile-quarkus-quickly.adoc:30
msgid "`-DskipTests` Skip maven surefire tests"
msgstr ""

#. type: Plain text
#: upstream/_versions/main/guides/includes/compile-quarkus-quickly.adoc:14
#: upstream/_versions/main/guides/includes/compile-quarkus-quickly.adoc:31
msgid "`-DskipITs` Skip maven failsafe integration tests"
msgstr ""

#. type: Plain text
#: upstream/_versions/main/guides/includes/compile-quarkus-quickly.adoc:15
#: upstream/_versions/main/guides/includes/compile-quarkus-quickly.adoc:32
msgid "`-Dno-test-modules` Do not build integration test modules"
msgstr ""

#.  tag::quickly-install-docs[]
#. type: Plain text
#: upstream/_versions/main/guides/includes/compile-quarkus-quickly.adoc:19
msgid "The following will build all modules in the Quarkus repository (except test modules) and install them in your local maven repository with the `999-SNAPSHOT` version:"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/includes/compile-quarkus-quickly.adoc:27
#, no-wrap
msgid ""
"$ ./mvnw clean install -Dquickly \\ #<1>\n"
"  -DskipTests \\ # <2>\n"
"  -DskipITs \\ # <3>\n"
"  -Dno-test-modules \\ # <4>\n"
"  -DskipDocs=false # <5>\n"
msgstr ""

#.  end::quickly-install-docs[]
#. type: Plain text
#: upstream/_versions/main/guides/includes/compile-quarkus-quickly.adoc:33
msgid "`-DskipDocs=false` re-enable the documentation build"
msgstr ""
