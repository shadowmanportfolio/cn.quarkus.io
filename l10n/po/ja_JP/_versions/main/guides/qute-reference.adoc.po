"{#for item in items}\n"
"  {item_count}. {item.name} <1>\n"
"  {#if item_hasNext}<br>{/if} <2>\n"
"{/each}\n"
msgstr ""

#. type: Plain text
#: upstream/_versions/main/guides/qute-reference.adoc:661
msgid "`item_count` represents one-based index."
msgstr ""

#. type: delimited block =
