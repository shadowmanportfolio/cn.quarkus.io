#, no-wrap
msgid ""
"    String render() {\n"
"       return msg.hello_name(\"Jachym\");\n"
"    }\n"
"}\n"
msgstr ""

#. type: Plain text
#: upstream/_versions/main/guides/qute-reference.adoc:2218
msgid "The annotation value is a locale tag string (IETF)."
msgstr "アノテーションの値はロケールタグ文字列(IETF)です。"

